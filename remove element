class Solution {
    public int removeElement(int[] nums, int val) {
        int index=0;//2
        for(int i=0;i<nums.length;i++){
            if(nums[i]==val){//i=0--3==3
            //i=1 2==3
                //remove

        }
        else if(nums[i]!=val){
            nums[index]=nums[i];//1 index-->2 // i=1-- 2==3
            index++;//2 3 2 3
                    // 2 2 --
            }
        }
        return index;
    }
}
