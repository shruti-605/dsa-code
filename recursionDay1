#include<iostream>
using namespace std;
void print(int n){
	if(n==0)
	   return ;
	//tail recursion
	//backward
	cout<<n<<endl;
	print(n-1);
	// head recursion
	//forward
}
int main(){
	int n;
	cin>>n;
	cout<<endl;
	print(n);
}
